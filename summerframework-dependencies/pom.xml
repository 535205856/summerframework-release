				<artifactId>platform-monitor-dependencies</artifactId>
				<version>${platform-monitor.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>com.bkjk.platform.summerframework</groupId>
				<artifactId>platform-configcenter-dependencies</artifactId>
				<version>${platform-configcenter.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>
