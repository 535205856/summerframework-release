	<artifactId>summerframework-dependencies</artifactId>
	<version>2.0.0.RELEASE</version>
	<name>summerframework-dependencies</name>
	<description>SummerFramework Dependencies</description>
	<packaging>pom</packaging>
	<properties>
		<main.basedir>${basedir}/../..</main.basedir>
		<platform-configcenter.version>2.0.0.RELEASE</platform-configcenter.version>
		<platform-webapi.version>2.0.0.RELEASE</platform-webapi.version>
		<platform-jobcenter.version>2.0.0.RELEASE</platform-jobcenter.version>
		<platform-openfeign.version>2.0.0.RELEASE</platform-openfeign.version>
		<platform-eureka.version>2.0.0.RELEASE</platform-eureka.version>
		<platform-mybatis.version>2.0.0.RELEASE</platform-mybatis.version>
		<platform-monitor.version>2.0.0.RELEASE</platform-monitor.version>
		<platform-common.version>2.0.0.RELEASE</platform-common.version>
		<spring-cloud.version>Finchley.SR2</spring-cloud.version>
		<micrometer.version>1.1.0</micrometer.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.springframework.cloud</groupId>
				<artifactId>spring-cloud-dependencies</artifactId>
				<version>${spring-cloud.version}</version>
				<type>pom</type>
				<scope>import</scope>
